{
  "ServiceName": "plantsuite-tenants",
  "Keycloak": {
    "ApiUrl": "",
    "Realm": "plantsuite",
    "AdminClientId": "ps-tenants-admin",
    "AdminClientSecret": "",
    "IntrospectionClientId": "ps-auth-introspection",
    "IntrospectionClientSecret": ""
  },
  "Database": {
    "MongoDb": {
      "ConnectionString": "",
      "CollectionPrefixTag": "ten",
      "DatabaseName": "ps-tenants",
      "VerneMQDatabaseName": "VerneMQ"
    },
    "Redis": {
      "ConnectionString": ""
    }
  },
  "JwtOptions": {
    "Audience": "ps-iot-portal",
    "Authority": "",
    "RequireHttpsMetadata": true
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:80"
      }
    }
  },
  "MessageBus": {
    "RabbitMQ": {
      "Host": "",
      "Port": 5672,
      "RetryCount": 3,
      "RetryInterval": 3,
      "User": "",
      "Password": "",
      "VirtualHost": "/"
    },
    "MQTT": {
      "User": "",
      "Password": ""
    }
  },
  "Observability": {
    "OtlpEndpoint": "http://dashboard.aspire.svc.cluster.local:4317",
    "Metrics": {
      "Enabled": true
    },
    "Tracing": {
      "Enabled": true,
      "SamplerProbability": 0.01
    },
    "Logging": {
      "Enabled": true
    }
  },
  "Licensing": {
    "LicensePath": "/etc/plantsuite/license.crt",
    "ExpirationWarningDays": 90
  },
  "PortalIot": "https://portal.plantsuite.com",
  "SMTP": {
    "Host": "smtp.office365.com",
    "Port": 587,
    "Email": "",
    "Password": ""
  }
}